include menu
.container-fluid
  .row(ng-repeat='post in posts')
    .col-md-6.col-md-offset-2
      .page-header
        h3
          a(href='/blog/{{post.id}}') {{post.title}}
        h6 by {{post.author}} - {{post.creationDate | date:'MM/dd/yyyy @ h:mma'}}
      div(marked='post.body')
